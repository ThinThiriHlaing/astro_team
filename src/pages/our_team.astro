<Layout title="our_team">
  <section class="our_team_section">
    <div class="container">
      <div class="card-list">
        <div class="card animate__animated">
          <div class="circle-image-profile">
            <img src="/assets/profile-1.jpg" class="inner" alt="p-1" />

            <div class="bar left"></div>
            <div class="bar right"></div>
          </div>

          <div class="name-info">
            <h4 class="name">TTH</h4>
            <h5 class="level">Level - 10</h5>
          </div>

          <div class="member-info">
            <div class="follower">
              <div class="icon">
                <svg
                  xmlns="http://www.w3.org/2000/svg"
                  width="16"
                  height="16"
                  fill="currentColor"
                  class="bi bi-person-fill"
                  viewBox="0 0 16 16"
                >
                  <path
                    d="M3 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1H3zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z"
                  ></path>
                </svg>
              </div>
              <div class="number">32</div>
            </div>
            <div style="border: 0.5px solid #dbdbdb"></div>
            <div class="point">
              <div class="icon">
                <svg
                  xmlns="http://www.w3.org/2000/svg"
                  width="16"
                  height="16"
                  fill="currentColor"
                  class="bi bi-star-fill"
                  viewBox="0 0 16 16"
